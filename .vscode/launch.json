{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "execPath",
      "args": ["--profile-temp", "--extensionDevelopmentPath=${workspaceFolder}"],
      "preLaunchTask": "npm: webpack",
      "smartStep": true,
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
